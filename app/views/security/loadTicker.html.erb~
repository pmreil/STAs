<script type="text/javascript">
	var d = new Date();
	var endDate = d.getDate() + '-' + (d.getMonth()+1) + '-' + (d.getFullYear());
	var startDate = d.getDate() + '-' + (d.getMonth()+1) + '-' + (d.getFullYear()-1);
	var embedCodeDate = d.getYear() + '-' + (d.getMonth()+1) + '-' + d.getDate();
	if (typeof(embedWikichart) != "undefined") {
		embedWikichart("http://charts.wikinvest.com/WikiChartMini.swf","chartContainer","470","250",{"embedCodeDate":embedCodeDate,"endDate":endDate,"startDate":startDate,"ticker":"<%=h @security.ticker %>","showAnnotations":"true","liveQuote":"true"},{});
	}
</script>

