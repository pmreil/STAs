<script type="text/javascript">
$(document).ready(function($) {
	$("#accordion").accordion({ collapsible:true,active:false,autoHeight:false });
	$('#fdbk_tab').click(function () { $('#chartContainer').hide(); });
	$('#fdbk_close').click(function () { $('#chartContainer').show(); });

  $('#latestNewsLink').click(function () { $('#latestNews').load('/security/loadGoogleNews/<%=u @security.ticker %>', function() { $("a[href^='http']").attr('target','_blank'); }); });
  $('#latestBlogsLink').click(function () { $('#latestBlogs').load('/security/loadGoogleBlog/<%=u @security.name %>', function() { $("a[href^='http']").attr('target','_blank'); }); });
  $('#latestTweetsLink').click(function () { $('#latestTweets').load('/security/loadTwitterFeed/<%=u @security.ticker %>', function() { $("a[href^='http']").attr('target','_blank'); }); });
  $('#commentsLink').click(function () { $.getScript('http://disqus.com/forums/stocktickerapps/embed.js'); });

});

</script>


